{
  "manifest_version": 2,
  "name": "Extension",
  "author": "AnOldStory",
  "version": "1.0.1",
  "description": "Extenstion",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["injection.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "React Ext"
  },
  "options_page": "index.html",
  "permissions": ["tabs", "storage", "http://*/", "https://*/"],
  "web_accessible_resources": ["js/libs/jquery.min.js"]
}
